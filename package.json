{
  "name": "crypto",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint && prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint:fix": "expo lint --fix && prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e:build:ios": "detox build --configuration ios.sim.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:e2e:build:ios:release": "detox build --configuration ios.sim.release",
    "test:e2e:ios:release": "detox test --configuration ios.sim.release --cleanup",
    "test:e2e:build:android": "detox build --configuration android.emu.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:build:android:release": "detox build --configuration android.emu.release",
    "test:e2e:android:release": "detox test --configuration android.emu.release --cleanup"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@legendapp/list": "^1.1.4",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.84.1",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "immer": "^10.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "use-debounce": "^10.0.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@testing-library/react-native": "^13.2.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "detox": "^20.40.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
